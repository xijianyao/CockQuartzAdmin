    <title>job管理</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="@(CockQuartzConst.StaticUrl)Eui/example1.1/favicon.ico?v=@MvcApplication.Version" rel="shortcut icon" />
    <link href="@(CockQuartzConst.StaticUrl)Eui/1.1/css/eui.min.css?v=@MvcApplication.Version" rel="stylesheet" />

    <link href="~/Content/loading/loading.css" rel="stylesheet" />
    @RenderSection("Header", false)
</head>
<body>
